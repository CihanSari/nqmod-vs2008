<GameData>

	<!-- Museums now need Amphitheaters instead of Opera Houses. They also provide +2 Culture by default. -->
	<Building_ClassesNeededInCity>
		<Update>
			<Set BuildingClassType="BUILDINGCLASS_AMPHITHEATER" />
			<Where BuildingType="BUILDING_MUSEUM" />
		</Update>
	</Building_ClassesNeededInCity>
	<Building_YieldChanges>
		<Update>
			<Set Yield="2" />
			<Where BuildingType="BUILDING_MUSEUM" YieldType="YIELD_CULTURE" />
		</Update>
	</Building_YieldChanges>

</GameData>