<GameData>
	
	<!-- Collectivization (replaces Young Pioneers): +3 Food and +1 Happiness in each City. -->
	<Policy_BuildingClassHappiness>
		<Delete PolicyType="POLICY_YOUNG_PIONEERS" />
	</Policy_BuildingClassHappiness>
	<Policies>
		<Update>
			<Set ExtraHappinessPerCity="1" />
			<Where Type="POLICY_YOUNG_PIONEERS" />
		</Update>
	</Policies>
	<Policy_CityYieldChanges>
		<Row>
			<PolicyType>POLICY_YOUNG_PIONEERS</PolicyType>
			<YieldType>YIELD_FOOD</YieldType>
			<Yield>3</Yield>
		</Row>
	</Policy_CityYieldChanges>
	
